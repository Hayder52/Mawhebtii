<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.connections.gui.VideoController">
    <stylesheets>
        <URL value="@video.css" />
    </stylesheets>
   <children>
      <MediaView fx:id="mv" fitHeight="200.0" fitWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="400.0" AnchorPane.topAnchor="0.0" />
      <HBox layoutY="300.0" prefHeight="100.0" prefWidth="302.0">
         <children>
            <Button fx:id="btn_play" mnemonicParsing="false" onMouseClicked="#on_click_btn_play" prefHeight="45.0" prefWidth="52.0" text="play">
               <HBox.margin>
                  <Insets top="3.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btn_stop" mnemonicParsing="false" onMouseClicked="#on_click_btn_stop" prefHeight="44.0" prefWidth="52.0" text="stop">
               <HBox.margin>
                  <Insets top="3.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="392.0" layoutY="305.0" prefHeight="81.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnlike" mnemonicParsing="false" onAction="#like" prefHeight="45.0" prefWidth="52.0" text="like" />
            <Button fx:id="btndislike" mnemonicParsing="false" onAction="#dislike" prefHeight="46.0" prefWidth="52.0" text="dislike" />
            <Button fx:id="btncomment" mnemonicParsing="false" onAction="#clickcommnt" prefHeight="46.0" prefWidth="75.0" text="comment" />
         </children>
      </HBox>
   </children>
</AnchorPane>
